{
  "rules": {
     "indent": 0,
     "prefer-const": 0,
     "no-unused-vars": 0,
     "eol-last": 0,
     "no-trailing-spaces": 0,
     "spaced-comment" : 0,
     "max-len": 0,
     "no-param-reassign": 0,
     "object-shorthand": 0,
     "arrow-body-style": 0,
     "object-curly-spacing" : 0,
     "key-spacing": 0,
     "no-console": 0,
     "func-names": 0,
     "no-extend-native": 0,
   //  "indent": [
   //    2,
   //    2,
   //    {
   //      "SwitchCase": 1
   //    }
   //  ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    // TODO: turn on later
    "comma-dangle": [
      0
    ]
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb/base"
  ]
}
